<?xml version="1.0" encoding="UTF-8" ?>
<FlyoutPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local1="clr-namespace:Soccer_App.View"
    x:Class="Soccer_App.View.MyFlyout"
    FlyoutLayoutBehavior="Split"
    BackgroundColor="#FDCB04"
    IconImageSource="hamburger.png"
    Shell.BackgroundColor="#FDCB04">
    <FlyoutPage.Flyout>
        <local1:FlyoutMenuPage x:Name="flyout"/>
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <local1:HomePage/>


            </x:Arguments>


        </NavigationPage>

    </FlyoutPage.Detail>
    
</FlyoutPage>




